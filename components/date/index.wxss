.container{
    width: 100%;
    height: 60rpx;
    display: inline-flex;
    flex-direction: row;
}
.index_container{
    display: flex;
    flex-direction: row;
    align-items: baseline;
}
.plain{
    font-size: 32rpx;
}
.index{
    font-size: 60rpx;
    line-height: 60rpx;
    font-weight: 600;
    margin-right: 14rpx;
}
.line{
    height: 44rpx;
    border-left: 1px solid #000;
    margin-right: 14rpx;
}

.date_container{
    display: flex;
    flex-direction: column;
    margin-top: 5rpx;
}
.month{
    font-size: 24rpx;
    /* line-height: 24rpx; */
}
.year{
    font-size: 20rpx;
}